                              <h4 class="panel-title">
                                <a data-toggle="collapse" data-parent="#accordion" href="#tab-custom">{{ settings.custom_tab_title }}</a>
                              </h4>
                            </div>
                            <div id="tab-custom" class="panel-collapse collapse">
                              <div class="panel-body">
                                {{ settings.custom_tab_content }}  	
                              </div>
                            </div>
                          </div>
                          {% endif %}
                          {% if settings.display_size_chart %}
