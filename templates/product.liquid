                            <div class="panel-heading">
                              <h4 class="panel-title">
                                <a data-toggle="collapse" data-parent="#accordion" href="#tab-size-chart">{{ settings.custom_size_chart_title }}</a>
                              </h4>
                            </div>
                            <div id="tab-size-chart" class="panel-collapse collapse">
